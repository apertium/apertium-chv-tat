  <mode name="chv-tat" install="yes">
    <pipeline>
      <program name="lt-proc -w">
        <file name="chv-tat.automorf.bin"/>
      </program>
      <program name="cg-proc  -n -w -1">
        <file name="chv-tat.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
